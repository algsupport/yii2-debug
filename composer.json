{
    "name": "algsupport/yii2-debug",
    "description": "The debugger extension for the Yii framework",
    "keywords": [
        "yii2",
        "debug",
        "debugger"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/algsupport/yii2-debug/issues",
        "source": "https://github.com/algsupport/yii2-debug"
    },
    "authors": [
        {
            "name": "Qiang Xue",
            "email": "qiang.xue@gmail.com"
        },
        {
            "name": "Simon Karlen",
            "email": "simi.albi@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.2",
        "ext-mbstring": "*",
        "algsupport/yii2": "*"
    },
    "autoload": {
        "psr-4": {
            "yii\\debug\\": "src"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
